<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textViewStyle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/maroon</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">8pt</item>
    </style>

    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar"></style>
    <style name="tema" parent="AppTheme">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:colorBackground">@color/darkred</item>
    </style>
</resources>